<?xml version="1.0" encoding="UTF-8"?>
<resources>
  <!--this comment will be parsed as header comment-->
  <string name="In a typical strings file the key is the text of the target language">the value holds the translated text</string>
  <!--an alternative is to use generic keys-->
  <string name="page.main.title">Titel der Hauptseite</string>
  <!--comments in .strings files
can be multi line,
single line
or combination of the two-->
  <string name="hello_user">Hello %1$s</string>
  <!--keys and values can be spread to multiple lines-->
  <string name="welcome.message">Welcome back, we have missed you</string>
  <!--comments belong to the next key value pair-->
  <string name="visit.count">this is your %1$d visit to our site</string>
</resources>
