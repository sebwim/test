// this comment will be parsed as header comment
"In a typical strings file the key is the text of the target language"="Der Wert enthält den übersetzten Text";

// an alternative is to use generic keys
"page.main.title"="Titel der Hauptseite";
/* comments in .strings files 
can be multi line, 
single line
or combination of the two */
"hello_user"="Hallo %1$s";

// keys and values can be spread to multiple lines
"welcome.message"="Willkommen zurück, wir haben Sie verpasst";

// comments belong to the next key value pair
"visit.count"="dies Ihr %1$d Besuch auf unserer Website";
